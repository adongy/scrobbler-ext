{
    "manifest_version": 2,
    "name": "Music Scrobbler",
    "version": "0.0.2",
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    },
    "permissions": [
        "nativeMessaging",
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "16": "icon/16.png",
        "32": "icon/32.png",
        "48": "icon/48.png",
        "128": "icon/128.png"
    },
    "key": "",
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Monitor songs"
    }
}
